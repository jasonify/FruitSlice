class Fruit extends MovieClip{	public function Fruit(x,y){		trace("fruit says hello");		_level0.createEmptyMovieClip("square_mc", this.getNextHighestDepth());				_level0.square_mc.loadMovie("orange.swf");		_level0.square_mc._x = x;		_level0.square_mc._y = y;	}		public function x(){		return _level0.square_mc._x;			}	public function y(){		return _level0.square_mc._y;	}		public function was_sliced(point_x, point_y, item_x, item_y, item_diameter){					if(point_x >= item_x and point_x <= item_x+item_diameter){			if(point_y >= item_y and point_y <= item_y + item_diameter){				return true;			}		}				return false;	}	};