class Fruit {		//private  	private var diameter:Number;	private var mv:MovieClip;	private var gravity:Number =  -9.8 ;		private var initial_y:Number;	private var initial_v:Number;	private var wind:Number;		private var txt:TextField;		public function Fruit(name, width, vi, wind, x,y){		trace("fruit says hello");		_level0.createEmptyMovieClip("fruit_"+name, _level0.getNextHighestDepth());				mv = _level0["fruit_"+name];		mv.loadMovie("orange.swf");		//mv._xscale = 50;		//smv._yscale = 50;		mv._x = x;		mv._y = y;		initial_y = y;		this.initial_v = vi;		this.wind = wind;				this.diameter = width;				_level0.createTextField("Heresometext"+name, _level0.getNextHighestDepth(), 2, 2, 200, 20);		txt = _level0["Heresometext"+name];		txt.text = "5";				var emphatic:TextFormat = new TextFormat();		emphatic.bold = true;		emphatic.size = 16;		emphatic.font = "Georgia";		txt.setTextFormat(emphatic);				txt._x = mv._x;		txt._y = mv._y;	}		public function x(){		return mv._x;			}	public function y(){		return mv._y;	}		public function update(time){				//Vi*T + g*t^2				var distance:Number =  (initial_v*150)*(1+time) + (gravity * (1+time/10) * (1+time));		mv._y = initial_y - distance/(1000 *1.2);		mv._x+=this.wind;				txt._x = mv._x;		txt._y = mv._y;																																															}	public function was_sliced(){					//_root._xmouse, _root._ymouse, fruit._x, fruit._y, 100		if(_root._xmouse >= this.x() and _root._xmouse <= this.x()+ diameter){			if(_root._ymouse >= this.y() and _root._ymouse <= this.y() + diameter){				mv.removeMovieClip();				txt.removeTextField();				return true;			}		}				return false;	}	};