class Fruit {		private var diameter:Number;	private var mv:MovieClip;	//private  	private var gravity:Number =  -9.8 ;		private var initial_y:Number;		public function Fruit(name, width, x,y){		trace("fruit says hello");		_level0.createEmptyMovieClip("fruit_"+name, _level0.getNextHighestDepth());				mv = _level0["fruit_"+name];		mv.loadMovie("orange.swf");		//mv._xscale = 50;		//smv._yscale = 50;		mv._x = x;		mv._y = y;		initial_y = y;				this.diameter = width;	}		public function x(){		return mv._x;			}	public function y(){		return mv._y;	}		public function update(time){				//Vi*T + g*t^2						var distance:Number =  (9.8*100)*(1+time) + (gravity * (1+time/10) * (1+time));		//distance *= -1;		trace(distance);		//trace(initial_y);		mv._y = initial_y - distance/1000;																																																			}	public function was_sliced(){					//_root._xmouse, _root._ymouse, fruit._x, fruit._y, 100		if(_root._xmouse >= this.x() and _root._xmouse <= this.x()+ diameter){			if(_root._ymouse >= this.y() and _root._ymouse <= this.y() + diameter){				mv.removeMovieClip();				return true;			}		}				return false;	}	};